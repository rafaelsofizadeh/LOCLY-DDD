    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/platform-express": "^8.2.3",
    "@sendgrid/mail": "^7.4.5",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.5",
